	var jqueryJsHost = (("https:" == document.location.protocol) ? "https://" : "http://");
	loadJS(jqueryJsHost + 'ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js');
	loadJS(jqueryJsHost + 'ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/jquery-ui.min.js'); //TODO: update to v1.8.6 and fix issues with dialogs(centered) and test other components
	function loadJS(src){
		var browserName = navigator.userAgent.toLowerCase();
		if(browserName.indexOf("msie") != -1 || browserName.indexOf('chrome') > -1 || browserName.indexOf('safari') > -1){
			document.write("<script type='text/javascript' src='"+src+"'><\/script>");
		}
		else {
			var scriptElem = document.createElement('script');
			scriptElem.src = src;
			document.getElementsByTagName('head')[0].appendChild(scriptElem);
		}
	}
</script>

