<script type="text/javascript">
	$(document).ready(function(){
		initPagination(1, 10, 22, '');
		shareOnTwitter();
		shareOnFB();
		showFilterBox();
	});		

	function initPagination(page, elementsPerPage ,listSize, filter) {
		var filterStr = "";
		if(filter != '') {
			filterStr = "&f="+filter;
		}
		listSize = parseInt(listSize);
		if(listSize > elementsPerPage){
			$("#pagination").pagination(listSize, {
				items_per_page: elementsPerPage,
				num_display_entries: 4,
				link_to: "http://" + window.location.host + window.location.pathname + "?p=__id__"+filterStr,
				current_page: page - 1,
				callback: function(p){}
		    });
		    $("#pagination a").each(function(){
		    	$(this).attr('rel', 'nofollow');
		    });
		}
	}

	function showFilterBox(){
		$("#filterBox").selectbox().bind('change', function(){
			document.location = $(this).val();
		});
		$(".greenSelect").css("visibility", "visible");
		
	}
	
	function shareOnEmail(subject, body){
		body = unescape(Utils.htmlDenc(body));
		body = escape(body);
		window.location.href="mailto:?Subject=" + subject + "&body=" + body;
	}
</script>

		








<script type="text/javascript" >
	$(document).ready(function(){
		$(".headerMenuLink").click(function(e) {
			var x = $(this).offset().left + "px";
			var y = ($(this).offset().top + 15) + "px";
			$("#headerMenu").css("left", x).css("top", y).slideToggle("fast"); 
			e.stopImmediatePropagation();
		});
		$("body").click(function(e){
			if ($("#headerMenu").height() > 0) $("#headerMenu").hide();
			e.stopImmediatePropagation();
		});
		
			$("#feedBackLink").click(function(){ Dialogs.showFeedBackDialog();});
			$("#howItWorksLink").click(function(e){ Dialogs.showHowItWorksDialog(e);});
		
		
		initMerchantCategoryMenu();
	});
	
	function initMerchantCategoryMenu() {
		if (typeof MerchantCategoryMenu == 'undefined') {
			var t = setTimeout("initMerchantCategoryMenu", 1000);
			return;
		}
		MerchantCategoryMenu.selectedCategoryId = '1192';

		
		
		
			
				MerchantCategoryMenu.loadCategoryMenu("categoryHeaderArea");
			
			
				MerchantCategoryMenu.loadCategoryMenu("categorySubHeaderArea");
			
		
	}
</script>
		








<script type="text/javascript">	
	var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www."); 
	document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
	
	var userDefined= 
			            
			             null 
			        ;
	var googleAnalyticsAccount = null;
	
		googleAnalyticsAccount = "UA-12608031-1";
	
	if (null != googleAnalyticsAccount) {
		try {
			var pageTracker = _gat._getTracker(googleAnalyticsAccount);
			var activeWhiteLabelMerchantHost = "spotlight.opentable.com";
			if (userDefined!=null) { pageTracker._setVar(userDefined); }
			pageTracker._trackPageview();
			if (activeWhiteLabelMerchantHost) {
				var merchantGaTrackingCode = "";
				if (merchantGaTrackingCode) {
					var pageTrackerFromMerchant = _gat._getTracker(merchantGaTrackingCode);
					pageTrackerFromMerchant._trackPageview();
				}
			}
		} catch (e) {}
	}
</script>
		
			










