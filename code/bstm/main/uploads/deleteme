<script type="text/javascript">
	$(document).ready(function(){
		$("#categoryId").val();
		$("#categoryId").selectbox();
		$(".greenSelect").css("visibility", "visible");
		$("#submitCityButton").click(function(){
			SubmitCity.submitClosestCity();
		});
		processTextAreaClickElement($("#submitCityEmailId"), "Email");

