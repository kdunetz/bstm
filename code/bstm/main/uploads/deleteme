	var WARNING_TITLE = "Warning";
	function getMicrositeUrlPrefix() {
		var url = "";
		return url;
	}
	var MICROSITE_URL_PREFIX = "";
	var ACTIVE_WHITE_LABEL_MERCHANT_ID = null;
	var MICROSITE_URL_PREFIX = getMicrositeUrlPrefix();
	
