<script>
	function addClass(element, value) {
		if(!element.className) {
			element.className = value;
		} else {
			newClassName = element.className;
			newClassName+= " ";
			newClassName+= value;
			element.className = newClassName;
		}
	}

	var activeTabId = document.getElementById('activeTabId').value; 
	if (activeTabId.length > 0) {
		addClass(document.getElementById(activeTabId), 'activeMainTabItem');
	}
</script>
