<script type="text/javascript">
function privacyDelayer(){
    window.location = "http://www.opentable.com/info/privacypolicy.aspx"
}
jQuery(document).ready(function(){

jQuery(".dealDetailsTitle").parent().hide();

// Deal Details Page text changes
 jQuery(".gbReviewsTitle h2").html("Reviews from OpenTable Diners");
 jQuery(".merchantDescTitle h2").html("Hours of Operation:");
 jQuery(".shareDealTitle").html("Share the Spotlight");
 jQuery("#privacyPolicy").attr("href", "http://www.opentable.com/info/privacypolicy.aspx");
 jQuery(".companySupportWebsiteBox br").hide();

// Deal Details Page module moving
jQuery('.gbReviews').insertBefore('#dealTalkSection');
jQuery('.prodDetailsTitle, .prodDetailsText').prependTo('.nimbleExtraInfoCol');

jQuery('.merchantDescTitle').eq(1).hide();
jQuery('.companyInfo').eq(1).hide();
jQuery('.merchantDescText').eq(1).hide();

// Buy Page text changes
 jQuery("#buyReviewTipText").html("Review your order above. When you click \"Complete Order\", your credit card will be charged.");
jQuery("#paymentInfoTitleHelp").html("(enter name as it appears on credit card or pay with PayPal.)");

