(
    [0] => 	function loadJS(src){
)
Array
(
    [0] => 		var browserName = navigator.userAgent.toLowerCase();
)
Array
(
    [0] => 		if(browserName.indexOf(\"msie\") != -1 || browserName.indexOf(\'chrome\') > -1 || browserName.indexOf(\'safari\') > -1){
)
Array
(
    [0] => 			document.write(\"<script type=\'text/javascript\' src=\'\"+src+\"\'><\\/script>\");
)
Array
(
    [0] => 		}
)
Array
(
    [0] => 		else {
)
Array
(
    [0] => 			var scriptElem = document.createElement(\'script\');
)
Array
(
    [0] => 			scriptElem.src = src;
)
Array
(
    [0] => 			document.getElementsByTagName(\'head\')[0].appendChild(scriptElem);
)
Array
(
    [0] => 		}
)
Array
(
    [0] => 	}
)
Array
(
    [0] => </script>
)
